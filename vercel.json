{
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    },
    {
      "src": "codecrisp-frontend/build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
   
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api",
      "dest": "index.js"
    },
    {
      "src": "/api/login",
      "dest": "index.js"
    },
    {
      "src": "/.*",
      "dest": "codecrisp-frontend/build/index.html"
    }
  ]
}