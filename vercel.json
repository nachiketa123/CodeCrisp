{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./codecrisp-frontend/build",    
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ]
  }