{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./codecrisp-frontend/build",    
        "use": "@vercel/static"
      },
      {
        "env": {
        "NODE_ENV": "production"
        }
    }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ]
  }